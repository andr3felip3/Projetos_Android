        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAro12"
        app:srcCompat="@drawable/b1" />

    <ImageButton
        android:id="@+id/imageButtonB5"
        android:layout_width="120dp"
        android:layout_height="105dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonB3"
        app:srcCompat="@drawable/b5" />

    <ImageButton
        android:id="@+id/imageButtonB3"
        android:layout_width="120dp"
        android:layout_height="105dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
